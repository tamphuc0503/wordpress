msgstr "Ponovite pozadinu"

#: config-layerslider/LayerSlider/assets/config/defaults.php:756
msgid "Global background image repeat."
msgstr "Globalna pozadinska slika se ponavlja."

#: config-layerslider/LayerSlider/assets/config/defaults.php:758
msgid "No-repeat"
msgstr "Nema ponavljanja"

#: config-layerslider/LayerSlider/assets/config/defaults.php:759
