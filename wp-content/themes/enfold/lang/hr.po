#: config-layerslider/LayerSlider/assets/views/transition-builder.php:128
#: config-layerslider/LayerSlider/assets/views/transition-builder.php:630
#: config-layerslider/LayerSlider/assets/wp/scripts_l10n.php:371
msgid "Remove transition"
msgstr "Ukloni prijelaz"

#: config-layerslider/LayerSlider/assets/views/transition-builder.php:108
msgid "No 2D transitions yet."
msgstr "Još nema 2D prijelaza."

#: config-layerslider/LayerSlider/assets/views/transition-builder.php:135
msgid "No 3D transitions yet."
msgstr "Još nema 3D prijelaza."

